{
  "name": "projecttemplatereact",
  "version": "1.0.0",
  "description": "A react template build structure for a webpack build",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && webpack --config webpack.production.js",
    "clean": "rimraf dist",
    "dev": "npm run dev:tailwind && webpack-dev-server --config webpack.development.js --open",
    "dev:tailwind": "npx tailwindcss build src/scss/tailwind/tailwind.css -o src/css/tailwind/tailwind.css && npm run minify:tailwind",
    "format": "npm run lint && npm run prettier",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "minify:tailwind": "cleancss src/css/tailwind/tailwind.css -o src/css/tailwind/tailwind.css --debug",
    "prettier": "prettier --write \"src/**/*.{html,htm,css,scss,json,js,jsx}\" --write",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorsuchtim/projectTemplate-react.git"
  },
  "keywords": [],
  "author": "Tim Gorsuch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gorsuchtim/projectTemplate-react/issues"
  },
  "homepage": "https://github.com/gorsuchtim/projectTemplate-react#readme",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/plugin-transform-shorthand-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-css-cli": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-react": "^7.20.5",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "clean-css": "^4.2.3",
    "node-sass": "^4.14.1",
    "tailwindcss": "^1.7.6"
  }
}
